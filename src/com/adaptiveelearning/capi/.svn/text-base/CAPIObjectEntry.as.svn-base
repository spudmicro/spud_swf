package com.adaptiveelearning.capi {
	
	public class CAPIObjectEntry	{
		
		public var parent:Object;
		public var targetPath:String;
		public var name:String;
		public var inspectable:Boolean;
		public var object:Object;
		
		
		public function CAPIObjectEntry(name:String,object:Object,parent:Object,targetPath:String,inspectable:Boolean) {
			this.name = name;
			this.object = object;
			this.parent = parent;
			this.targetPath = targetPath;
			this.inspectable = inspectable;		
		}

	}
}